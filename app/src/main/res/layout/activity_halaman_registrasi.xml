<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="halamanRegistrasiVM" type="com.nadyasapplication.app.modules.halamanregistrasi.data.viewmodel.HalamanRegistrasiVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearHalamanRegistr"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <ImageView android:id="@+id/imageBackOne"
            android:layout_height="@dimen/_21pxh"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_38pxv"
            android:layout_width="@dimen/_40pxh"
            android:scaleType="fitXY" android:src="@drawable/img_back1"
            tools:ignore="ContentDescription" tools:src="@drawable/img_back1"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnayobergabungb"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_38pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_8pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:id="@+id/txtAyobergabungb"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_2pxh"
                android:text="@{halamanRegistrasiVM.halamanRegistrasiModel.txtAyobergabungb}"
                style="@style/txtJosefinSansromanregular18" tools:text="@string/msg_ayo_bergabung_b"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDonatebloods"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{halamanRegistrasiVM.halamanRegistrasiModel.txtDonatebloods}"
                style="@style/txtJosefinSansromanlight13" tools:text="@string/msg_donate_blood_s"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearNamaLengkap"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_52pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNamaLengkap"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{halamanRegistrasiVM.halamanRegistrasiModel.txtNamaLengkap}"
                    style="@style/txtJosefinSansromanmedium13" tools:text="@string/lbl_nama_lengkap"/>
                <EditText
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_2"
                    android:ellipsize="end"
                    android:hint="@string/lbl_nama_lengkap"
                    android:id="@+id/etGroupTwelve"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_9pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:text="@={halamanRegistrasiVM.halamanRegistrasiModel.etGroupTwelveValue}"
                    android:textColorHint="@color/gray_800_7f"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearNoHp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNoHp"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{halamanRegistrasiVM.halamanRegistrasiModel.txtNoHp}"
                    style="@style/txtJosefinSansromanmedium13" tools:text="@string/lbl_no_hp"/>
                <EditText
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_2"
                    android:ellipsize="end"
                    android:hint="@string/lbl_0813_xxxx_xxxx"
                    android:id="@+id/etPrice" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_11pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:text="@={halamanRegistrasiVM.halamanRegistrasiModel.etPriceValue}"
                    android:textColorHint="@color/gray_800_7f"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAlamatEmail"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEmail"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{halamanRegistrasiVM.halamanRegistrasiModel.txtEmail}"
                    style="@style/txtJosefinSansromanmedium13" tools:text="@string/lbl_email"/>
                <EditText
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_2"
                    android:ellipsize="end"
                    android:hint="@string/msg_gmail_com_mai"
                    android:id="@+id/etGroupFourteen"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_9pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@={halamanRegistrasiVM.halamanRegistrasiModel.etGroupFourteenValue}"
                    android:textColorHint="@color/gray_800_7f"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearPassword"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtKataSandi"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{halamanRegistrasiVM.halamanRegistrasiModel.txtKataSandi}"
                    style="@style/txtJosefinSansromanmedium13" tools:text="@string/lbl_kata_sandi"/>
                <EditText
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_2"
                    android:ellipsize="end" android:hint="@string/msg"
                    android:id="@+id/etPassword"
                    android:inputType="textPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_8pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@={halamanRegistrasiVM.halamanRegistrasiModel.etPasswordValue}"
                    android:textColorHint="@color/gray_800_7f"
                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearTgllahir"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_3pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:id="@+id/txtTanggalLahir"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_weight="0.19"
                    android:layout_width="0dp"
                    android:text="@{halamanRegistrasiVM.halamanRegistrasiModel.txtTanggalLahir}"
                    style="@style/txtJosefinSansromanregular13" tools:text="@string/lbl_tanggal_lahir"/>
                <FrameLayout android:id="@+id/frameStackcalenderone"
                    android:layout_height="@dimen/_36pxv"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_69pxh" style="@style/groupStylebluegray_2">
                    <ImageView android:id="@+id/imageCalenderOne"
                        android:layout_gravity="top|end"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_4pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_calender1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_calender1"/>
                </FrameLayout>
                <LinearLayout android:id="@+id/linearRowberatbadan"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.53"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStylecornerRadius_3">
                    <LinearLayout android:gravity="end"
                        android:id="@+id/linearColumnberatbadan"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_weight="0.61"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtBeratBadan"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{halamanRegistrasiVM.halamanRegistrasiModel.txtBeratBadan}"
                            style="@style/txtJosefinSansromanregular13" tools:text="@string/lbl_berat_badan"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtKg"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{halamanRegistrasiVM.halamanRegistrasiModel.txtKg}"
                            style="@style/txtJosefinSansromanregular13" tools:text="@string/lbl_kg"/>
                    </LinearLayout>
                    <View
                        android:background="@drawable/rectangle_bg_bluegray_100_radius_2"
                        android:id="@+id/viewRectangleThirty"
                        android:layout_height="@dimen/_36pxv"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_49pxh"/>
                </LinearLayout>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnBergabung"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_31pxh"
                android:layout_marginStart="@dimen/_31pxh"
                android:layout_marginTop="@dimen/_65pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_13pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_13pxv"
                android:singleLine="true"
                android:text="@string/lbl_bergabung"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_2" tools:text="@string/lbl_bergabung"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowlanguage"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_32pxh"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.83"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{halamanRegistrasiVM.halamanRegistrasiModel.txtLanguage}"
                    style="@style/txtJosefinSansromanextralight12" tools:text="@string/msg_sudah_memiliki"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMasuk"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_weight="0.17"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{halamanRegistrasiVM.halamanRegistrasiModel.txtMasuk}"
                    style="@style/txtJosefinSansromanregular12_2" tools:text="@string/lbl_masuk2"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
