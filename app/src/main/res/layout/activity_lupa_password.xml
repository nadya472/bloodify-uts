<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="lupaPasswordVM" type="com.nadyasapplication.app.modules.lupapassword.data.viewmodel.LupaPasswordVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearLupaPassword"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <ImageView android:id="@+id/imageBackOne"
            android:layout_height="@dimen/_21pxh"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_37pxv"
            android:layout_width="@dimen/_40pxh"
            android:scaleType="fitXY" android:src="@drawable/img_back1"
            tools:ignore="ContentDescription" tools:src="@drawable/img_back1"/>
        <ImageView android:id="@+id/imageAsset2319xThree"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_87pxh"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_35pxv"
            android:layout_width="@dimen/_100pxh"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_asset2319x3}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_asset2319x3"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumntulisalamatem"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_32pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtTulisalamatem"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{lupaPasswordVM.lupaPasswordModel.txtTulisalamatem}"
                style="@style/txtJosefinSansromanregular12_3" tools:text="@string/msg_tulis_alamat_em"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAlamatEmail"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_42pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEmail"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{lupaPasswordVM.lupaPasswordModel.txtEmail}"
                    style="@style/txtJosefinSansromanmedium13" tools:text="@string/lbl_email"/>
                <EditText
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_2"
                    android:ellipsize="end"
                    android:hint="@string/msg_gmail_com_mai"
                    android:id="@+id/etGroupTwentyNine"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_9pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@={lupaPasswordVM.lupaPasswordModel.etGroupTwentyNineValue}"
                    android:textColorHint="@color/gray_800_7f"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowbatal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_61pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnBatal"
                    android:layout_height="@dimen/_49pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="@dimen/_111pxh"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_16pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_batal"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_batal"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnUbah"
                    android:layout_height="@dimen/_49pxv"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_111pxh"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_16pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_ubah"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_ubah"/>
            </LinearLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtMASUK"
                android:layout_gravity="end"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_99pxh"
                android:layout_marginStart="@dimen/_99pxh"
                android:layout_marginTop="@dimen/_142pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{lupaPasswordVM.lupaPasswordModel.txtMASUK}"
                style="@style/txtJosefinSansromanmedium20_1" tools:text="@string/lbl_masuk"/>
        </LinearLayout>
    </LinearLayout>
</layout>
