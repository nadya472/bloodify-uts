<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="getStartedFiveVM" type="com.nadyasapplication.app.modules.getstartedfive.data.viewmodel.GetStartedFiveVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumn"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearIPhone82"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <LinearLayout android:gravity="end"
                android:id="@+id/linearColumndownloaderone"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_50">
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowdownloaderthree"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_68pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/linearRowdownloaderone"
                        android:layout_height="wrap_content"
                        android:layout_weight="-9.1"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageDownloaderOne"
                            android:layout_height="@dimen/_230pxh"
                            android:layout_marginBottom="@dimen/_20pxv"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_downloader1}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_downloader1"/>
                        <ImageView android:id="@+id/imageDownloaderTwo"
                            android:layout_height="@dimen/_270pxh"
                            android:layout_marginStart="@dimen/_34pxh"
                            android:layout_width="@dimen/_1pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_downloader2"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_downloader2"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageDownloaderThree"
                        android:layout_height="@dimen/_230pxh"
                        android:layout_marginBottom="@dimen/_30pxv"
                        android:layout_marginStart="@dimen/_394pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_1pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_downloader3"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_downloader3"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowdapatkanreward"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/linearRowmulaigayahidu"
                        android:layout_height="wrap_content"
                        android:layout_weight="-1.43"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <TextView android:id="@+id/txtMulaiGayaHidu"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="2.0"
                            android:layout_width="0dp"
                            android:text="@{getStartedFiveVM.getStartedFiveModel.txtMulaiGayaHidu}"
                            style="@style/txtOswaldregular16" tools:text="@string/msg_mulai_gaya_hidu"/>
                        <TextView android:id="@+id/txtMerekaSELAMAT"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_34pxv"
                            android:layout_marginStart="@dimen/_189pxh"
                            android:layout_weight="-1.0"
                            android:layout_width="0dp"
                            android:text="@{getStartedFiveVM.getStartedFiveModel.txtMerekaSELAMAT}"
                            style="@style/txtOswaldregular14" tools:text="@string/msg_mereka_selamat"/>
                    </LinearLayout>
                    <TextView android:id="@+id/txtDapatkanREWARD"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginStart="@dimen/_467pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_weight="2.43"
                        android:layout_width="0dp"
                        android:text="@{getStartedFiveVM.getStartedFiveModel.txtDapatkanREWARD}"
                        style="@style/txtOswaldregular14" tools:text="@string/msg_dapatkan_reward"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowkumpulkanpoint"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowlanguage"
                        android:layout_height="wrap_content"
                        android:layout_weight="-1.71"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <TextView android:id="@+id/txtLanguage"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_20pxv"
                            android:layout_weight="1.78"
                            android:layout_width="0dp"
                            android:letterSpacing="0.06"
                            android:text="@{getStartedFiveVM.getStartedFiveModel.txtLanguage}"
                            style="@style/txtQuestrialregular11" tools:text="@string/msg_temukan_jadwal"/>
                        <TextView android:id="@+id/txtDescription"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_178pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="-0.78"
                            android:layout_width="0dp"
                            android:letterSpacing="0.06"
                            android:text="@{getStartedFiveVM.getStartedFiveModel.txtDescription}"
                            style="@style/txtQuestrialregular11" tools:text="@string/msg_tahukan_kamu_ba"/>
                    </LinearLayout>
                    <TextView android:id="@+id/txtKumpulkanpoint"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_27pxv"
                        android:layout_marginStart="@dimen/_464pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="2.71"
                        android:layout_width="0dp"
                        android:letterSpacing="0.06"
                        android:text="@{getStartedFiveVM.getStartedFiveModel.txtKumpulkanpoint}"
                        style="@style/txtQuestrialregular11" tools:text="@string/msg_kumpulkan_point"/>
                </LinearLayout>

                <com.asksira.loopingviewpager.indicator.CustomShapePagerIndicator
                    android:id="@+id/indicatorSignal"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_113pxv"
                    android:layout_width="wrap_content"
                    app:drawable_normal="@drawable/indicator_unselected_bg_red_200_radius_8"
                    app:drawable_selected="@drawable/indicator_selected_bg_red_400_radius_8"
                    app:indicator_size="@dimen/_10pxh" app:indicator_spacing="@dimen/_2pxh"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
